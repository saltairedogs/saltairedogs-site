<svg viewBox="0 0 400 300" xmlns="http://www.w3.org/2000/svg">
  <rect width="400" height="300" fill="#f1f5f9"/>
  
  <!-- Border Collie style dog -->
  <g transform="translate(200, 150)">
    <!-- Body -->
    <ellipse cx="0" cy="15" rx="55" ry="35" fill="#2D3748"/>
    <ellipse cx="0" cy="20" rx="45" ry="25" fill="white"/>
    
    <!-- Head -->
    <circle cx="-25" cy="-25" r="32" fill="#2D3748"/>
    <ellipse cx="-25" cy="-20" rx="25" ry="20" fill="white"/>
    
    <!-- Ears -->
    <ellipse cx="-45" cy="-40" rx="10" ry="20" fill="#2D3748" transform="rotate(-20 -45 -40)"/>
    <ellipse cx="-5" cy="-40" rx="10" ry="20" fill="#2D3748" transform="rotate(20 -5 -40)"/>
    
    <!-- Black markings on face -->
    <ellipse cx="-35" cy="-30" rx="8" ry="12" fill="#2D3748"/>
    <ellipse cx="-15" cy="-30" rx="8" ry="12" fill="#2D3748"/>
    
    <!-- Eyes -->
    <circle cx="-35" cy="-28" r="3" fill="white"/>
    <circle cx="-15" cy="-28" r="3" fill="white"/>
    <circle cx="-35" cy="-28" r="2" fill="#2D3748"/>
    <circle cx="-15" cy="-28" r="2" fill="#2D3748"/>
    
    <!-- Nose -->
    <ellipse cx="-25" cy="-15" rx="2.5" ry="2" fill="#2D3748"/>
    
    <!-- Mouth -->
    <path d="M -25 -12 Q -20 -8 -15 -12" stroke="#2D3748" stroke-width="1.5" fill="none"/>
    <path d="M -25 -12 Q -30 -8 -35 -12" stroke="#2D3748" stroke-width="1.5" fill="none"/>
    
    <!-- Legs -->
    <rect x="-10" y="45" width="6" height="20" fill="#2D3748" rx="3"/>
    <rect x="-30" y="45" width="6" height="20" fill="white" rx="3"/>
    <rect x="30" y="45" width="6" height="20" fill="#2D3748" rx="3"/>
    <rect x="50" y="45" width="6" height="20" fill="white" rx="3"/>
    
    <!-- Tail -->
    <path d="M 50 5 Q 75 -15 80 10 Q 70 20 60 15" fill="#2D3748"/>
    <path d="M 55 8 Q 72 -8 75 12" fill="white"/>
  </g>
  
  <!-- Park bench in background -->
  <rect x="50" y="200" width="80" height="8" fill="#8B4513" rx="2"/>
  <rect x="52" y="175" width="4" height="25" fill="#8B4513"/>
  <rect x="124" y="175" width="4" height="25" fill="#8B4513"/>
  
  <!-- Trees in background -->
  <circle cx="350" cy="180" r="25" fill="#22C55E"/>
  <rect x="345" y="180" width="10" height="30" fill="#8B4513"/>
</svg>