<svg viewBox="0 0 400 300" xmlns="http://www.w3.org/2000/svg">
  <rect width="400" height="300" fill="#f8fafc"/>
  
  <!-- Happy golden retriever style dog -->
  <g transform="translate(200, 150)">
    <!-- Body -->
    <ellipse cx="0" cy="20" rx="60" ry="40" fill="#D4A574"/>
    
    <!-- Head -->
    <circle cx="-30" cy="-20" r="35" fill="#D4A574"/>
    
    <!-- Ears -->
    <ellipse cx="-50" cy="-35" rx="12" ry="25" fill="#C4955F" transform="rotate(-30 -50 -35)"/>
    <ellipse cx="-10" cy="-35" rx="12" ry="25" fill="#C4955F" transform="rotate(30 -10 -35)"/>
    
    <!-- Eyes -->
    <circle cx="-40" cy="-25" r="4" fill="#2D3748"/>
    <circle cx="-20" cy="-25" r="4" fill="#2D3748"/>
    <circle cx="-39" cy="-26" r="1.5" fill="white"/>
    <circle cx="-19" cy="-26" r="1.5" fill="white"/>
    
    <!-- Nose -->
    <ellipse cx="-30" cy="-10" rx="3" ry="2" fill="#2D3748"/>
    
    <!-- Mouth -->
    <path d="M -30 -7 Q -25 -2 -20 -7" stroke="#2D3748" stroke-width="2" fill="none"/>
    <path d="M -30 -7 Q -35 -2 -40 -7" stroke="#2D3748" stroke-width="2" fill="none"/>
    
    <!-- Tongue -->
    <ellipse cx="-30" cy="-2" rx="8" ry="4" fill="#FF69B4" opacity="0.8"/>
    
    <!-- Legs -->
    <rect x="-15" y="50" width="8" height="25" fill="#D4A574" rx="4"/>
    <rect x="-35" y="50" width="8" height="25" fill="#D4A574" rx="4"/>
    <rect x="25" y="50" width="8" height="25" fill="#D4A574" rx="4"/>
    <rect x="45" y="50" width="8" height="25" fill="#D4A574" rx="4"/>
    
    <!-- Tail -->
    <path d="M 55 10 Q 80 -10 85 15 Q 75 25 65 20" fill="#D4A574"/>
    
    <!-- Collar -->
    <rect x="-45" y="-5" width="30" height="6" fill="#DC2626" rx="3"/>
    <circle cx="-30" cy="-2" r="3" fill="#FCD34D"/>
  </g>
  
  <!-- Grass elements -->
  <path d="M 50 280 Q 55 270 60 280" stroke="#22C55E" stroke-width="2" fill="none"/>
  <path d="M 80 285 Q 85 275 90 285" stroke="#22C55E" stroke-width="2" fill="none"/>
  <path d="M 320 285 Q 325 275 330 285" stroke="#22C55E" stroke-width="2" fill="none"/>
  <path d="M 350 280 Q 355 270 360 280" stroke="#22C55E" stroke-width="2" fill="none"/>
</svg>