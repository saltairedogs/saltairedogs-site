<svg viewBox="0 0 400 300" xmlns="http://www.w3.org/2000/svg">
  <rect width="400" height="300" fill="#ecfdf5"/>
  
  <!-- Small terrier style dog -->
  <g transform="translate(200, 170)">
    <!-- Body -->
    <ellipse cx="0" cy="10" rx="40" ry="25" fill="#8B4513"/>
    
    <!-- Head -->
    <circle cx="-20" cy="-15" r="25" fill="#8B4513"/>
    
    <!-- Ears -->
    <ellipse cx="-35" cy="-30" rx="8" ry="15" fill="#654321"/>
    <ellipse cx="-5" cy="-30" rx="8" ry="15" fill="#654321"/>
    
    <!-- Eyes -->
    <circle cx="-28" cy="-18" r="3" fill="#2D3748"/>
    <circle cx="-12" cy="-18" r="3" fill="#2D3748"/>
    <circle cx="-27" cy="-19" r="1" fill="white"/>
    <circle cx="-11" cy="-19" r="1" fill="white"/>
    
    <!-- Nose -->
    <ellipse cx="-20" cy="-8" rx="2" ry="1.5" fill="#2D3748"/>
    
    <!-- Mouth -->
    <path d="M -20 -5 Q -16 -2 -12 -5" stroke="#2D3748" stroke-width="1.5" fill="none"/>
    <path d="M -20 -5 Q -24 -2 -28 -5" stroke="#2D3748" stroke-width="1.5" fill="none"/>
    
    <!-- Legs -->
    <rect x="-8" y="30" width="5" height="15" fill="#8B4513" rx="2"/>
    <rect x="-25" y="30" width="5" height="15" fill="#8B4513" rx="2"/>
    <rect x="25" y="30" width="5" height="15" fill="#8B4513" rx="2"/>
    <rect x="35" y="30" width="5" height="15" fill="#8B4513" rx="2"/>
    
    <!-- Tail (wagging up) -->
    <path d="M 35 0 Q 50 -20 45 -5" stroke="#8B4513" stroke-width="4" fill="none"/>
    
    <!-- Collar with tag -->
    <rect x="-30" y="-5" width="20" height="4" fill="#DC2626" rx="2"/>
    <circle cx="-20" cy="-3" r="2" fill="#FCD34D"/>
  </g>
  
  <!-- Flower -->
  <g transform="translate(100, 250)">
    <circle cx="0" cy="0" r="3" fill="#FCD34D"/>
    <ellipse cx="-5" cy="-2" rx="2" ry="4" fill="#F472B6" transform="rotate(-30)"/>
    <ellipse cx="5" cy="-2" rx="2" ry="4" fill="#F472B6" transform="rotate(30)"/>
    <ellipse cx="0" cy="-5" rx="4" ry="2" fill="#F472B6"/>
    <ellipse cx="0" cy="3" rx="4" ry="2" fill="#F472B6"/>
    <rect x="-0.5" y="3" width="1" height="8" fill="#22C55E"/>
  </g>
  
  <!-- Butterfly -->
  <g transform="translate(320, 100)">
    <ellipse cx="-3" cy="-2" rx="4" ry="6" fill="#A855F7" opacity="0.8"/>
    <ellipse cx="3" cy="-2" rx="4" ry="6" fill="#EC4899" opacity="0.8"/>
    <ellipse cx="-3" cy="2" rx="3" ry="4" fill="#A855F7" opacity="0.6"/>
    <ellipse cx="3" cy="2" rx="3" ry="4" fill="#EC4899" opacity="0.6"/>
    <line x1="0" y1="-6" x2="0" y2="6" stroke="#2D3748" stroke-width="1"/>
  </g>
</svg>